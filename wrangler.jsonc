{
    "$schema": "https://json.schemastore.org/wrangler",
    "name": "rar-website",
    // Use a stable compatibility date
    "compatibility_date": "2025-12-17",
    "main": "./workers/app.ts",
    // Smart placement for optimal performance
    "placement": {
        "mode": "smart"
    },
    // Enable observability/logging
    "observability": {
        "enabled": true
    },
    // KV Namespace bindings
    "kv_namespaces": [
        {
            "binding": "RAR_WEBSITE"
        }
    ]
}