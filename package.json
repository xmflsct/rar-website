    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "stripe": "^10.15.0",
    "tailwindcss": "^3.2.1",
