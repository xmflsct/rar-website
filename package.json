    "@types/lodash": "^4.14.184",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
