{
  "name": "rar-website",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css && remix build",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --kv RAR_WEBSITE --binding $(cat .env)",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^16.0.7",
    "@contentful/rich-text-react-renderer": "^15.19.0",
    "@contentful/rich-text-types": "^16.3.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.17",
    "@myparcel/constants": "^1.1.1",
    "@myparcel/sdk": "^3.3.1",
    "@remix-run/cloudflare": "^2.2.0",
    "@remix-run/cloudflare-pages": "^2.2.0",
    "@remix-run/react": "^2.2.0",
    "@stripe/stripe-js": "^2.1.11",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "i18n-iso-countries": "^7.7.0",
    "isbot": "^3.7.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-day-picker": "^8.9.1",
    "react-dom": "^18.2.0",
    "schema-dts": "^1.1.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@remix-run/dev": "2.2.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/lodash": "^4.14.201",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "stripe": "^14.4.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "wrangler": "^3.15.0"
  },
  "packageManager": "yarn@4.1.0"
}
